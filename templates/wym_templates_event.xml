  <template name="pageJsEvent" value="&lt;script type=&quot;text/javascript&quot; src=&quot;${ctx}/static/jquery/plugin/jquery.tmpl.js&quot;&gt;&lt;/script&gt;&#10;&lt;script type=&quot;text/javascript&quot; src=&quot;${ctx}/static/jquery/plugin/jquery.pagination.js&quot;&gt;&lt;/script&gt;&#10;&lt;script type=&quot;text/javascript&quot; src=&quot;${ctx}/static/jquery/event/ajaxpage-jqplugin.js&quot;&gt;&lt;/script&gt;&#10;&#10;&lt;!-- 分页开始--&gt;&#10;&lt;script type=&quot;text/x-jquery-tmpl&quot; id=&quot;$TMPL_ID$&quot;&gt;&#10;    &lt;ul class=&quot;clearfix&quot;&gt;&#10;&#9;&#9;{{each(i,obj) records}}&#10;&#9;&#9;&#9;    &lt;li id=&quot;work{{= obj.id}}&quot; style=&quot;cursor:pointer;&quot; &gt;&#10;&#9;&#9;&#9;        &lt;a href=&quot;/w2i/whereru/detail?userId={{= obj.id}}&quot; style = &quot;color:#771d1f;&quot;&gt;&#10;&#9;&#9;&#9;        {{if currentPage==1&amp;&amp;i==0}}&#10;&#9;&#9;&#9;            &lt;span class=&quot;rank&quot;&gt;&lt;img src=&quot;http://event51.wanmei.com/w2i/201510/lost/images/ranking1.png&quot; width=&quot;78&quot; height=&quot;68&quot; /&gt;&lt;/span&gt;&#10;&#9;&#9;&#9;        {{else currentPage==1&amp;&amp;i==1}}&#10;&#9;&#9;&#9;            &lt;span class=&quot;rank&quot;&gt;&lt;img src=&quot;http://event51.wanmei.com/w2i/201510/lost/images/ranking2.png&quot; width=&quot;78&quot; height=&quot;68&quot; /&gt;&lt;/span&gt;&#10;&#9;&#9;&#9;        {{else currentPage==1&amp;&amp;i==2}}&#10;&#9;&#9;&#9;            &lt;span class=&quot;rank&quot;&gt;&lt;img src=&quot;http://event51.wanmei.com/w2i/201510/lost/images/ranking3.png&quot; width=&quot;78&quot; height=&quot;68&quot; /&gt;&lt;/span&gt;&#10;&#9;&#9;&#9;        {{else}}&#10;&#9;&#9;&#9;            &lt;span class=&quot;rank&quot;&gt;&lt;strong&gt;{{= (currentPage-1)*10+i+1}}&lt;/strong&gt;&lt;/span&gt;&#10;&#9;&#9;&#9;        {{/if}}&#10;                        &lt;span class=&quot;name&quot;&gt;&lt;strong&gt;{{= obj.rolename}}&lt;/strong&gt;&lt;/span&gt;&#10;                        &lt;span class=&quot;id&quot;&gt;&lt;strong&gt;{{= obj.id}}&lt;/strong&gt;&lt;/span&gt;&#10;                        &lt;span class=&quot;server&quot;&gt;&lt;strong&gt;{{= obj.server}}&lt;/strong&gt;&lt;/span&gt;&#10;                        &lt;span class=&quot;zanNum&quot;&gt;&lt;strong&gt;{{= obj.ballot}}&lt;/strong&gt;&lt;/span&gt;&#10;                    &lt;/a&gt;&#10;                        &lt;span class=&quot;detaile&quot;&gt;&lt;a href=&quot;/w2i/whereru/detail?userId={{= obj.id}}&quot;&gt;&lt;/a&gt;&lt;/span&gt;&#10;                &lt;/li&gt;&#10;&#9;&#9;{{/each}}&#10;    &lt;/ul&gt;&#10;&#9;&lt;/script&gt;&#10;&lt;script type=&quot;text/javascript&quot;&gt;&#10;    var paginationOptions = {&#10;        pageSize: 10,&#10;        current_page: 0,&#10;        data_area_id: &quot;$DIV_DATA_ID$&quot;, //数据区&#10;        tmpl_script: &quot;$TMPL_ID$&quot;, //模板ID&#10;        page_area_id: &quot;$DIV_PAGE_ID$&quot;, //页码区&#10;        num_display_entries: 10,&#10;        num_edge_entries: 0,&#10;        prev_text: &quot;上一页&quot;,&#10;        next_text: &quot;下一页&quot;,&#10;        link_to: &quot;javascript:void(0);&quot;,&#10;        ellipse_text: &quot;&quot;,&#10;        afterLoad:function(){&#10;            $AFTER_LOAD_FUNCTION$ //after Load Function&#10;        }&#10;    };&#10;     //刷新列表&#10;    function initPagination(){&#10;        paginationOptions.url =&quot;${ctx}/$game$/$event$/$interface$&quot;;&#10;        $.hd.pagination(paginationOptions);&#10;    }&#10;    /*&#10;     &lt;div id=&quot;data&quot;&gt;&lt;/div&gt;&#10;     &lt;div id=&quot;page&quot;&gt;&lt;/div&gt;&#10;    */&#10;&lt;/script&gt;&#10;&lt;%--分页结束--%&gt;" description="pagenation for Template in Js" toReformat="true" toShortenFQNames="true">
