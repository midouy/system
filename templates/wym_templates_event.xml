  <template name="loginEvent" value="&lt;%--登录模块--%&gt;&#10;&lt;script type=&quot;text/javascript&quot; src=&quot;${ctx}/extend/wmpassport2/wanmei.passport.js&quot;&gt;&lt;/script&gt;&#10;&lt;script type=&quot;text/javascript&quot;&gt;&#10;    var username = &quot;${sessionScope.USER.account}&quot;;&#10;    function isEmpty(str) {&#10;        return str == null || str == &quot;&quot;;&#10;    }&#10;    function login() {&#10;        wanmei.passport.islogin({&#10;            session: 'USER',&#10;            _true: function () {&#10;            },&#10;            _false: function () {&#10;                wanmei.passport.login({&#10;                    session: 'USER',&#10;                    hdid: '$Event_Status$'&#10;                })&#10;            }&#10;        });&#10;    }&#10;    function logout() {&#10;        wanmei.passport.logout({&#10;            opacity: 60,&#10;            session: 'USER'&#10;        });&#10;    }&#10;    function initLoginLogout() {&#10;        $(&quot;#$Login_Button$&quot;).click(function () {&#10;            login();&#10;        });&#10;        $(&quot;#$LogOunt_Button$&quot;).click(function () {&#10;            logout();&#10;        });&#10;        if (isEmpty(username)) {&#10;            $('#$Logout_box$').hide();&#10;            $('#$Login_box$').show();&#10;        } else {&#10;            $('#$Login_box$').hide();&#10;            $('#$Logout_box$').show();&#10;            $(&quot;#$User_Name_ID$&quot;).html(username);&#10;        }&#10;    }&#10;    $(function () { //初始化登录模块&#10;        initLoginLogout();&#10;    })&#10;    /*&#10;    &lt;div&gt;&#10;        &lt;div id=&quot;Login_box&quot;&gt;&lt;div id = &quot;Login_Button&quot;&gt;&lt;/div&gt;&lt;/div&gt;&#10;        &lt;div id=&quot;Logout_box&quot;&gt;&lt;div id = &quot;Logout_Button&quot;&gt;&lt;/div&gt;&lt;/div&gt;&#10;    &lt;/div&gt;&#10;    */&#10;&lt;/script&gt;" description="Login JS" toReformat="true" toShortenFQNames="true">
